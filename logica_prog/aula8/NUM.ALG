Algoritmo "NUM"


Var
 n:inteiro

Inicio
escreval("---NUM---")
escreval("Digite n: ")
leia(n)
se(n<10)entao
escreva("n < 10")
        senao
             se((n>=10)e (n<50))entao
             escreva("n>=10 e n<50")
                         senao
                              escreva("n>=50")
             fimse
fimse
Fimalgoritmo